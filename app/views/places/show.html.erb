<h1><%= @place.name %>

<h2>Posts</h2>
<ul>
<% for posts in @place.posts %>
    <li>
        <h3><%= @posts.title %></h3>
        <h5><%= @posts.posted_on %></h5>
        <h4><%= @posts.description%></h4>        
    </li>
<% end %>
</ul>

<a href = "/posts/new?place_id=<%= @place.id %>">Create a New Post for This Place!</a>
<br><br>
<a href = "/places">Back to Places</a>